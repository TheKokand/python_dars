# maxsus metodlar
class Avto:
  __num_avto=0
  """avto classi"""
  def __init__(self,make,model,rang,yil,narh):
    """avtoning xususiyatlari"""
    self.make=make
    self.model=model
    self.rang=rang
    self.yil=yil
    self.narh=narh
    Avto.__num_avto+=1

  def __repr__(self):
    """obyekt haqida malumot"""
    return f"Avto: {self.rang} {self.make} {self.model}"
# print(dir(Avto))    

# 
avto1 = Avto("GM","Malibu","Qora",2020,40000)
print(avto1) # obyekt haqida ma'lumot 

